{
  "env": {
    "browser": true
  },
  "extends": "airbnb",
  "parserOptions": {
    sourceType: "module",
    "ecmaFeatures": {
      "jsx": true
    },
  },
  "rules": {
    "no-var": "error",
    "global-require": "off",
    "no-param-reassign": "off",
    "no-use-before-define": ["error", { "functions": false, "classes": true }],
    "no-console": "off",
    "yoda": "off"
  }
}
